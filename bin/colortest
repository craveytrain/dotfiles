#!/bin/bash

for color in {0..15} ; do
	if [ ${#color} == 1 ]; then 
		spacer="   "
	else
		spacer="  "
	fi
	echo -en "\033[48;5;${color}m  \033[0m${spacer}\033[38;5;${color}m${color}\033[0m  "
	if [ $((($color + 1) % 8)) == 0 ] ; then
		echo #New line
	fi
done; echo

for color in {16..231} ; do
	if [ ${#color} == 2 ]; then 
		spacer="  "
	else
		spacer=" "
	fi
	echo -en "\033[48;5;${color}m  \033[0m${spacer}\033[38;5;${color}m${color}\033[0m  "
	if [ $((($color + 3) % 6)) == 0 ] ; then
		echo #New line
		if [ $((($color + 21) % 36)) == 0 ] ; then
			echo #New line
		fi
	fi
done; echo

for color in {232..255} ; do
	echo -en "\033[48;5;${color}m  \033[0m \033[38;5;${color}m${color}\033[0m  "
done; echo

exit 0